! clean up urxvt, add keybinds
URxvt.intensityStyles: false
URxvt.keysym.Shift-Up: command:\033]720;1\007
URxvt.keysym.Shift-Down: command:\033]721;1\007
URxvt.resize-font.smaller: C-down
URxvt.resize-font.higher: C-up
URxvt.iso14755: false
URxvt.iso14755_52: false

! URxvt config
URxvt.scrollstyle: plain
URxvt.font: xft:FiraCode:size=12
URxvt.boldFont: xft:FiraCode-Bold:size=12
URxvt*.shading: 1

! terminal padding
URxvt.internalBorder: 15

! make otf/ttf fonts look good
Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintfull
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb


! Scrollbar
*scrollBar: false

! And scrolling
*scrollTtyOutput: false
*scrollWithBuffer: true
*scrollTtyKeypress: true

!URLs
*underlineURLs: true

! clickable links, clipboard support, mouse wheel support
! fly font resizing
URxvt.perl-ext-common: default,clipboard,vtwheel
URxvt.url-launcher: /usr/bin/firefox

! clipboard
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

st.font: FiraCode-12:pixelsize=17:antialias=true:autohint=true;
st.cursorshape: 2
st.cursorthickness: 1

! colors
*.background: #080808
*.foreground: #808080
