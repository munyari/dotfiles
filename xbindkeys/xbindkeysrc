"xbacklight -dec 5"
  XF86MonBrightnessDown

"xbacklight -inc 5"
  XF86MonBrightnessUp

"amixer -c 1 set Master 5+"
  XF86AudioRaiseVolume

"amixer -c 1 set Master 5-"
  XF86AudioLowerVolume

"amixer -c 1 set Master toggle"
  XF86AudioMute

"scrot -e 'mkdir -p ~/images/shots && mv $f ~/images/shots'"
  Print

"scrot -ue 'mkdir -p ~/images/shots && mv $f ~/images/shots'"
  Control + Print

"scrot -se 'mkdir -p ~/images/shots && mv $f ~/images/shots'"
  Alt + Alt_R + Print

"firefox"
  Mod4 + Super_L + f

"passmenu"
  Mod4 + Super_L + p

"dmenu-frecency"
  Mod4 + Super_L + d

"urxvt"
  Mod4 + Super_L + d + Return
